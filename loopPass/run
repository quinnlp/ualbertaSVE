CC=clang CXX=clang++ cmake --build  .
clang -c -g -emit-llvm test/fun.c -o test/fun.bc
clang -S -g -emit-llvm test/fun.c -o test/fun.ll
opt -O3 -opt-bisect-limit=396 test/fun.bc -o test/temp.bc
opt -load SVELoopPass.dylib -SVELoopPass test/temp.bc -o /dev/null
rm -f test/temp.bc

